{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "npm start tours",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/tours"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ]
}